<odoo>
    <!-- View slide.channel.partner form -->
    <record id="view_slide_channel_partner_form" model="ir.ui.view">
        <field name="name">slide.channel.partner.form</field>
        <field name="model">slide.channel.partner</field>
        <field name="inherit_id" ref="studio_customization.default_form_view_fo_d3e862be-1d34-4da2-9253-d87c87a94da9"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='channel_website_id']">
                <field name="skill_acquired_ids"/>
                <tree>
                    <field name="acquired"/>
                </tree>
                <form>
                    <field name="acquired"/>
                </form>
            </xpath>
        </field>
    </record>
</odoo>
