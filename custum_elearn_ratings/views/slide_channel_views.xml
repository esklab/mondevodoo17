<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- View slide.channel form -->
    <record id="view_slide_channel_form" model="ir.ui.view">
        <field name="name">slide.channel.view.form</field>
        <field name="model">slide.channel</field>
        <field name="inherit_id" ref="website_slides.view_slide_channel_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='visibility']" position="after">
                <group name="Moyenne du Cours" string="string">
                    <field name="x_average_rating"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>